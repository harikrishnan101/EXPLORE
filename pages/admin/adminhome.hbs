    </tr>
     {{#each data}}
    <tr style="width: 100px; height: 100px; "  class="text-center " >
        <td class=> 1</td>
        <td>{{this.createdAt}}</td>
        <td >{{this.content}}</td>
        <td class="text-center mt-5"><button class="btn btn-danger " onclick="deletePost('{{this._id}}')">delete</button></td>
    </tr>
   {{/each}}
